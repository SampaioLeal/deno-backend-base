{
  "imports": {
    "hono": "jsr:@hono/hono@^4.5.9",
    "zod": "https://deno.land/x/zod@v3.23.8/mod.ts",
    "@prisma/client": "npm:@prisma/client@5.19.0",
    "@std/assert": "jsr:@std/assert@1.0.3"
  },
  "tasks": {
    "start": "deno run -A main.ts",
    "test": "deno test -A ./__tests__",
    "prisma:generate": "deno run -A npm:prisma@latest generate --no-engine --schema=./database/prisma/schema.prisma"
  },
  "compilerOptions": {
    "jsx": "precompile",
    "jsxImportSource": "hono/jsx"
  }
}
